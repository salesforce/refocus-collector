    "validator": "^7.1.0",
    "vm2": "^3.4.6",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "jscs": "^3.0.7",
    "mocha": "^3.4.1",
