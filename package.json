{
  "name": "refocus-collector",
  "version": "1.0.0",
  "description": "Collector system to automate feeding data in to the Refocus platform.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "watch": {
    "start": {
      "patterns": [
        "src"
      ],
      "extensions": "yaml",
      "quiet": true
    }
  },
  "scripts": {
    "start" : "node index.js",
    "watch": "npm-watch",
    "jscs": "./node_modules/jscs/bin/jscs src test",
    "test": "npm run jscs && ./node_modules/.bin/mocha -R dot --recursive ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salesforce/refocus-collector.git"
  },
  "author": "Salesforce CX Tools",
  "license": "BSD-3-Clause",
  "keywords": [
    "refocus",
    "collector",
    "collectors",
    "refocus-collector"
  ],
  "bugs": {
    "url": "https://github.com/salesforce/refocus-collector/issues"
  },
  "homepage": "https://github.com/salesforce/refocus-collector#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "compression": "^1.6.2",
    "debug": "^2.6.8",
    "errors": "^0.3.0",
    "express": "^4.15.3",
    "fs": "0.0.1-security",
    "js-yaml": "^3.8.4",
    "nock": "^9.0.13",
    "notevil": "^1.1.0",
    "path": "^0.12.7",
    "repeat": "0.0.6",
    "superagent": "^3.5.2",
    "superagent-mocker": "^0.5.2",
    "validator": "^7.1.0",
    "swagger-tools": "^0.10.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "jscs": "^3.0.7",
    "mocha": "^3.4.1",
    "mock-fs": "^4.4.1",
    "sinon": "^2.3.4",
    "npm-watch": "^0.2.0"
  }
}
