    "joi": "^10.6.0",
    "just-template": "^1.1.22",
    "nock": "^9.0.13",
    "path": "^0.12.7",
    "repeat": "0.0.6",
    "simple-oauth2": "^1.3.0",
    "superagent": "^3.5.2",
